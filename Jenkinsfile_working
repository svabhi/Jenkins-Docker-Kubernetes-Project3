pipeline {
    agent any
	tools {
		maven 'Maven'
	}
	
	environment {
		PROJECT_ID = 'geometric-hull-355406'
                CLUSTER_NAME = 'k8s-cluster'
                LOCATION = 'us-central1-c'
                CREDENTIALS_ID = 'kubernetes'
		
                
						
	}
	
    stages {
	    stage('Scm Checkout') {
		    steps {
			    checkout scm
		    }
	    }
	    
	    stage('test') {
		    steps {
			    sh 'gcloud version'
				sh 'gcloud compute zones list'
		    }
	    }
	    
	    
	        
    }
}
